# Environment variables template
# Copy this file to .env and replace with your actual values

# Database connection for DigitalOcean Postgres
# Get this from your DigitalOcean database dashboard
DATABASE_URL="postgresql://username:password@your-cluster-host:25060/defaultdb?sslmode=require"

# Optional: Node environment
NODE_ENV="development"

# Optional: Server port (defaults to 4000)
PORT=4000