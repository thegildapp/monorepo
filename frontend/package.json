{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run check && vite --host 0.0.0.0 --port 5173",
    "build": "npm run check && vite build",
    "check": "npm run copy-schema && relay-compiler && tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "preview": "vite preview",
    "relay": "npm run copy-schema && relay-compiler",
    "copy-schema": "cp ../schema.graphql ./schema.graphql 2>/dev/null || cp schema.graphql ./schema.graphql 2>/dev/null || true"
  },
  "dependencies": {
    "graphql": "^16.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-relay": "^20.0.0",
    "react-router-dom": "^7.6.3",
    "relay-runtime": "^20.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-relay": "^18.2.1",
    "@types/relay-runtime": "^19.0.2",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "relay-compiler": "^20.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-plugin-relay": "^2.1.0"
  }
}
